            return
        else:

            if platform.node() == "mint":
                fname, filter = PySide.QtWidgets.QFileDialog.getSaveFileName(None, "Save snapshot to text file", setPathLatestDirectory, " (*.FCMacro);; (*.py);; (*.txt);; (*.*);; (*)")#PySide Mint
                filter = filter[filter.find("."):filter.find(")")]
